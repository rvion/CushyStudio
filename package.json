{
    "name": "cushystudio",
    "displayName": "[ALPHA] Cushy Studio",
    "type": "module",
    "author": {
        "name": "rvion",
        "url": "https://github.com/rvion/CushyStudio",
        "email": "vion.remi@gmail.com"
    },
    "icon": "public/CushyLogo.png",
    "publisher": "rvion",
    "repository": {
        "type": "git",
        "url": "https://github.com/rvion/CushyStudio"
    },
    "description": "ðŸŽ¨ Generative Art Web-IDE and embeddable typescript SDK - based on ComfyUI",
    "license": "AGPL-3.0",
    "version": "0.0.99",
    "engines": {},
    "categories": [
        "Other"
    ],
    "main": "./main.js",
    "contributes": {
        "configuration": {
            "title": "CushyStudio",
            "properties": {
                "cushystudio.serverHostHTTP": {
                    "type": [
                        "string"
                    ],
                    "default": "http://localhost:8188",
                    "description": "your ComfyUI http (or https) server host"
                },
                "cushystudio.serverWSEndoint": {
                    "type": [
                        "string"
                    ],
                    "default": "ws://localhost:8188/ws",
                    "description": "your ComfyUI ws (or wss) endpoint"
                }
            }
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "cushy-studio",
                    "title": "Cushy Studio",
                    "icon": "public/CushyLogo.png"
                }
            ]
        },
        "views": {
            "cushy-studio": [
                {
                    "id": "cushystudio-servers",
                    "name": "Servers"
                },
                {
                    "id": "cushyrun",
                    "name": "Cushy Runs"
                },
                {
                    "id": "cushygraphs",
                    "name": "CushyGraph"
                }
            ]
        },
        "commands": [
            {
                "command": "cushystudio.helloWorld",
                "title": "cushystudio.helloWorld | Hello world"
            },
            {
                "command": "cushystudio.updateschema",
                "title": "cushystudio.updateschema | Connect"
            },
            {
                "command": "cushystudio.openwebview",
                "title": "cushystudio.openwebview | Open webview"
            },
            {
                "command": "cushystudio.import",
                "title": "cushystudio.import | import flow"
            },
            {
                "command": "cushystudio.importlegacy",
                "title": "cushystudio.importlegacy | import flow (preserve legacy IDs)"
            }
        ]
    },
    "scripts": {
        "ðŸ“Œ FRONT": "",
        "front:shell": "electron shell",
        "front:watch": "vite build --watch",
        "front:dev": "vite dev",
        "front:build": "vite build",
        "front:preview": "vite preview",
        "front:inspect": "npx vite-bundle-visualizer",
        "ðŸ“Œ CHECKS": "",
        "tsc:compile": "tsc -p ./",
        "tsc:watch": "tsc -watch -p ./",
        "tsc:check": "tsc",
        "ðŸ“Œ RELEASE": "",
        "bump": "bump patch --commit=v --tag=v --push",
        "ðŸ“Œ DEV": "",
        "js:old": "--experimental-specifier-resolution=node --experimental-modules",
        "js": "node --loader @swc-node/register/esm",
        "jswatch": "node --loader @swc-node/register/esm --watch",
        "ðŸ“Œ CORE": "",
        "sync": "yarn js src/core/ComfySpec.fetch.ts",
        "lib": "yarn js src/core/Comfy.gen.ts",
        "ðŸ“Œ COMPILER": "",
        "old:compile": "yarn js src/compiler/entry.gen.ts",
        "old:sample": "yarn js src/compiler/entry.fetch.ts",
        "old:test": "yarn js src/compiler/test.ts",
        "old:test2": "yarn js src/compiler/test2.ts",
        "decl": "yarn js src/typings/sdkGenerator.ts",
        "decl:local": "yarn js src/sdk/sdkGenerator.ts",
        "decl:list": "tsc -p tsconfig.decl.json --listFiles --noEmit | grep /Users/loco/dev/CushyStudio/src"
    },
    "dependencies": {
        "@electron/remote": "2.0.10",
        "@lexical/react": "^0.11.1",
        "@rsuite/icons": "1.0.2",
        "@types/react-mentions": "^4.1.8",
        "chokidar": "^3.5.3",
        "cors": "^2.8.5",
        "cytoscape": "3.25.0",
        "cytoscape-klay": "3.1.4",
        "electron": "25.2.0",
        "esbuild": "0.18.8",
        "express": "^4.18.2",
        "form-data": "4.0.0",
        "highlight.js": "^11.8.0",
        "lexical": "^0.11.1",
        "marked": "5.1.0",
        "mobx": "6.9.0",
        "mobx-react-lite": "3.4.3",
        "nanoid": "4.0.2",
        "node-fetch": "3.3.1",
        "open": "^9.1.0",
        "pathe": "1.1.1",
        "PostCSS": "npm:postcss@8.4.24",
        "prettier": "^2.8.8",
        "react": "18.2.0",
        "react-dnd": "^16.0.1",
        "react-dnd-html5-backend": "^16.0.1",
        "react-dom": "18.2.0",
        "react-mentions": "4.4.9",
        "react-zoom-pan-pinch": "3.0.8",
        "reactflow": "11.7.4",
        "rsuite": "5.35.1",
        "ws": "8.13.0",
        "y-leveldb": "^0.1.2",
        "y-websocket": "^1.5.0",
        "yet-another-react-lightbox": "3.11.2",
        "yjs": "13.6.5"
    },
    "devDependencies": {
        "@swc-node/register": "1.6.6",
        "@types/cors": "^2.8.13",
        "@types/cytoscape": "3.19.10",
        "@types/cytoscape-klay": "3.1.1",
        "@types/express": "^4.17.17",
        "@types/glob": "8.1.0",
        "@types/marked": "5.0.0",
        "@types/node": "20.3.1",
        "@types/prettier": "2.7.3",
        "@types/react": "18.2.14",
        "@types/react-dom": "18.2.6",
        "@types/vscode": "1.79.1",
        "@types/ws": "8.5.5",
        "@typescript-eslint/eslint-plugin": "5.60.0",
        "@typescript-eslint/parser": "5.60.0",
        "@vitejs/plugin-react": "4.0.1",
        "autoprefixer": "^10.4.14",
        "cross-env": "^7.0.3",
        "eslint": "8.43.0",
        "glob": "10.3.0",
        "less": "4.1.3",
        "postcss": "8.4.24",
        "tailwindcss": "^3.3.2",
        "typescript": "5.1.3",
        "vite": "4.3.9"
    }
}
