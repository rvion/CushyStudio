{
    "name": "cushystudio",
    "displayName": "[ALPHA] Cushy Studio",
    "type": "module",
    "author": {
        "name": "rvion",
        "url": "https://github.com/rvion/CushyStudio",
        "email": "vion.remi@gmail.com"
    },
    "icon": "public/CushyLogo.png",
    "publisher": "rvion",
    "repository": {
        "type": "git",
        "url": "https://github.com/rvion/CushyStudio"
    },
    "description": "üé® Generative Art Web-IDE and embeddable typescript SDK - based on ComfyUI",
    "license": "AGPL-3.0",
    "version": "0.0.99",
    "categories": [
        "Other"
    ],
    "main": "./lib/index.js",
    "typings": "./lib/index.d.ts",
    "scripts": {
        "build-css:tsx-only": "unocss \"src/**/*.tsx\" -o release/uno.css",
        "build-css": "unocss \"src/**/*.tsx\" \"src/**/*.ts\" -o release/uno.css",
        "pretty": "prettier --write .",
        "eslint-debug-rules": "eslint --print-config file.js > eslint.config.debug.json && code eslint.config.debug.json",
        "eslint-debug-state": "eslint --debug --no-color src/ -o eslint-status.txt",
        "eslint-fixall": "eslint src/ --fix",
        "eslint-fixall2": "./node_modules/.bin/format-imports --config import-sorter.json -e tsx src/**/*.ts",
        "eslint-fixall3": "./node_modules/.bin/format-imports --config import-sorter.json -e tsx src/**/*.tsx",
        "postinstall": "patch-package",
        "üìå LEGACY": "-----------------------------------------------------------------------",
        "start": "CUSHY_RUN_MODE=dist node src/shell/start.cjs",
        "start-dev": "CUSHY_RUN_MODE=dev node src/shell/start.cjs",
        "dev": "CUSHY_RUN_MODE=dev node src/shell/start.cjs",
        "run-update": "CUSHY_RUN_MODE=dev node src/shell/update.cjs",
        "run-install": "CUSHY_RUN_MODE=dev node src/shell/install.cjs",
        "üìå CODEGEN": "----------------------------------------------------------------------",
        "gen-lezer": "lezer-generator ./src/prompt/grammar/.grammar -o ./src/prompt/grammar/grammar.parser.ts",
        "gen-bins-node": "npm rebuild",
        "gen-supa-types": "supabase gen types typescript --schema public --linked > src/supa/database.types.ts",
        "gen-supa-types-full": "supabase gen types typescript --schema public --schema auth --linked > src/supa/database.types.ts",
        "gen-bins": "electron-builder install-app-deps",
        "gen-manager": "bun src/manager/_utils/_downloadComfyManagerJSONs.ts",
        "gen-manager:watch": "bun --watch src/manager/_utils/_downloadComfyManagerJSONs.ts --skip-download",
        "gen-assets": "bun src/utils/assets/assets.gen.ts",
        "gen-dts": "tsc -p tsconfig.doc.json",
        "gen-syms": "node src/shell/externals.cjs",
        "gen-doc": "typedoc --excludeInternal --out site/documentation src/back/Runtime.ts src/controls/Builder.ts src/controls/Widget.ts",
        "gen-virtual": "bun src/utils/codegen/gen.initial-schema.ts",
        "supa:migrate": "supabase migration up --linked",
        "üìå TEST": "------------------------------------------------------------------------",
        "test": "bun test src",
        "test:watch": "bun test --watch src",
        "üìå FRONT": "------------------------------------------------------------------------",
        "build": "node src/shell/build.js",
        "front:shell": "electron src/shell",
        "front:watch": "vite build --watch",
        "front:dev": "vite dev",
        "front:build": "vite build",
        "front:preview": "vite preview",
        "front:inspect": "npx vite-bundle-visualizer",
        "üìå RELEASE": "----------------------------------------------------------------------",
        "bump": "bump patch --commit=v --tag=v --push",
        "diff-with-master-all": "git diff master                   > tmp/diff-with-master-all.diff && code tmp/diff-with-master-all.diff",
        "diff-with-master-add": "git diff master | grep -Ev '^\\-' > tmp/diff-with-master.diff     && code tmp/diff-with-master.diff",
        "diff-with-dev-all": "   git diff dev                      > tmp/diff-with-dev-all.diff    && code tmp/diff-with-dev-all.diff",
        "diff-with-dev-add": "   git diff dev | grep -Ev '^\\-'    > tmp/diff-with-dev.diff        && code tmp/diff-with-dev.diff",
        "diff-local": "cd library/local && git diff master > doc.diff && code doc.diff",
        "üìå UTILS": "------------------------------------------------------------------------",
        "pics": "bun --watch src/tools/autoupload-screenshots.ts",
        "cycles:madge": "madge --circular src/**/*.tsx src/**/*.ts",
        "cycles:skott": "skott -nit -m raw -s",
        "üìå TSC": "-------------------------------------------------------------------------",
        "tsc:perf": "bun ./src/perfs/monitorTSCPerfs",
        "tsc:peft0": "tsc -p tsconfig.perf.json --generateTrace tmp/traceDir --noEmit  && npx @typescript/analyze-trace tmp/traceDir --forceMillis=100 > x.txt",
        "vscode:tsc:debug": "bun src/perfs/monitorVSCodePerfs.ts",
        "patch-vscode-typescript": "./src/scripts/patchTypesHoverTruncationMac.sh",
        "üìå LINT": "-------------------------------------------------------------------------",
        "lint": "./node_modules/.bin/eslint src --ext .ts,.tsx",
        "lint:fix": "./node_modules/.bin/eslint src --ext .ts,.tsx --fix",
        "üìå FORM": "-------------------------------------------------------------------------",
        "form:transpile": "   tsc -p tsconfig.doc.json",
        "form:ls": "          madge lib/controls/Builder.loco.js",
        "form:deps:‚è∏Ô∏è": "     ./src/scripts/show-file-deps.sh src/controls/Builder.loco.ts",
        "form:deps": "        madge lib/controls/Builder.loco.js -i tmp/depedencies.png && code tmp/depedencies.png",
        "form:deps:npm": "    madge lib/controls/Builder.loco.js --include-npm -i tmp/depedencies.png && code tmp/depedencies.png",
        "form:deps-svg": "    madge lib/controls/Builder.loco.js -i tmp/depedencies.svg && open tmp/depedencies.svg",
        "form:deps-svg:npm": "madge lib/controls/Builder.loco.js --include-npm -i tmp/depedencies.svg && code tmp/depedencies.svg",
        "form:export": " rsync -r $CUSHY_INSTALL_DIR/@cushy/forms/ $MONOLOCO_INSTALL_DIR/src/cushy-forms/ --exclude=node_modules --exclude=pre --exclude=.vscode --exclude=global.d.ts --exclude=rollup.config.mjs --exclude=tsconfig.tsbuildinfo --exclude=main.js --exclude=SUMMARY.txt --delete --exclude=.gitignore --exclude=README.md --exclude=main.css --exclude=main.d.ts --exclude=main.meta.json --exclude=main.tw.css --exclude=package-lock.json --exclude=package.json --exclude=dts/main.d.rollup-stats.json --exclude=tsconfig.json --exclude=tailwind.config.js --exclude=import-sorter.json",
        "tests:export": "rsync -r $CUSHY_INSTALL_DIR/src/csuite $MONOLOCO_INSTALL_DIR/src/cushy-forms/src --include='*/' --include='*.test.ts' --include='csuite/model/TESTS/utils/*' --exclude='*'",
        "form:build": "       bun src/csuite-bundler/bundle-csuite.ts",
        "form:bundle-dts": "  './src/scripts/node_modules/.bin/rollup' -c './src/scripts/form.rollup.config.mjs'",
        "form:bundle-dts2": " yarn form:bundle-dts && cp stats.json "
    },
    "dependencies": {
        "@codemirror/autocomplete": "^6.18.1",
