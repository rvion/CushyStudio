{
    "name": "cushystudio",
    "publisher": "rvion",
    "description": "ğŸ¨ Generative Art Web-IDE and embeddable typescript SDK - based on ComfyUI",
    "private": true,
    "engines": {
        "vscode": "^1.77.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:*.cushy.ts"
    ],
    "main": "./out/main.js",
    "contributes": {
        "customEditors": [
            {
                "viewType": "cushystudio.run",
                "displayName": "",
                "selector": [
                    {
                        "filenamePattern": "*.cushy.OLD"
                    }
                ]
            }
        ],
        "configuration": {
            "title": "CushyStudio",
            "properties": {
                "cushystudio.useCodeSnippetsOnMethodSuggest": {
                    "type": "boolean",
                    "default": false,
                    "description": "Complete functions with their parameter signature."
                },
                "cushystudio.tsdk": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "default": null,
                    "description": "Specifies the folder path containing the tsserver and lib*.d.ts files to use."
                }
            }
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "cushy-studio",
                    "title": "Cushy Studio",
                    "icon": "resources/CushyLogo.png"
                }
            ]
        },
        "views": {
            "cushy-studio": [
                {
                    "id": "cushystudio-servers",
                    "name": "Servers"
                },
                {
                    "id": "cushyrun",
                    "name": "Cushy Runs"
                },
                {
                    "id": "cushygraphs",
                    "name": "CushyGraph"
                }
            ]
        },
        "commands": [
            {
                "command": "cushystudio.openwebview",
                "title": "Hello World (React + Vite): Show"
            },
            {
                "command": "cushystudio.helloWorld",
                "title": "Hello World"
            },
            {
                "command": "cushystudio.runcurrentscript",
                "title": "run current script"
            },
            {
                "command": "cushystudio.connect",
                "title": "Connect to ComfyUI"
            },
            {
                "command": "cushystudio.samplewebview",
                "title": "Hello World"
            },
            {
                "command": "cushystudio.openjs",
                "title": "Open JS"
            }
        ]
    },
    "author": {
        "name": "rvion",
        "url": "https://github.com/rvion/CushyStudio",
        "email": "vion.remi@gmail.com"
    },
    "license": "unlicensed",
    "version": "0.0.64",
    "scripts": {
        "vscode:prepublish": "pnpm run esbuild-base --minify",
        "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
        "esbuild": "pnpm run esbuild-base -- --sourcemap",
        "esbuild-watch": "pnpm run esbuild-base --sourcemap --watch",
        "test-compile": "tsc -p ./",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "pnpm run compile && pnpm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js",
        "[RELEASE]": "--------------------------------------------",
        "bump": "bump patch --commit=v --tag=v --push",
        "logo": "./scripts/derive-assets.sh",
        "[UI]": "-------------------------------------------------",
        "vite:dev": "          vite",
        "vite:build": "        vite build",
        "vite:preview": "      vite preview",
        "check-bundle-size": " npx vite-bundle-visualizer",
        "[CHECKS]": "-------------------------------------------------",
        "check": "tsc",
        "site:typecheck": "tsc",
        "[DEV]": " ğŸ‰ğŸ‰ğŸ‰ğŸ‰ğŸ‰ğŸ‰ğŸ‰ğŸ‰ğŸ‰ğŸ‰ğŸ‰ğŸ‰ğŸ‰ğŸ‰ğŸ‰ğŸ‰ğŸ‰ğŸ‰ğŸ‰ğŸ‰ğŸ‰ğŸ‰",
        "js": "node --loader ts-node/esm --experimental-specifier-resolution=node --experimental-modules",
        "web": "open -n -a \"/Applications/Google Chrome.app/Contents/MacOS/Google Chrome\" --args --user-data-dir=\"/tmp/chrome_dev_test\" --disable-web-security",
        "[CORE]": "ğŸ‘ğŸ‘ğŸ‘ğŸ‘ğŸ‘ğŸ‘ğŸ‘ğŸ‘ğŸ‘ğŸ‘ğŸ‘ğŸ‘ğŸ‘ğŸ‘ğŸ‘ğŸ‘ğŸ‘ğŸ‘ğŸ‘ğŸ‘ğŸ‘ğŸ‘",
        "sync": "yarn js src/core/ComfySpec.fetch.ts",
        "lib": "yarn js src/core/Comfy.gen.ts",
        "[COMPILER]": "ğŸ°ğŸ°ğŸ°ğŸ°ğŸ°ğŸ°ğŸ°ğŸ°ğŸ°ğŸ°ğŸ°ğŸ°ğŸ°ğŸ°ğŸ°ğŸ°ğŸ°ğŸ°ğŸ°ğŸ°",
        "old:compile": "yarn js src/compiler/entry.gen.ts",
        "old:sample": "yarn js src/compiler/entry.fetch.ts",
        "old:test": "yarn js src/compiler/test.ts",
        "old:test2": "yarn js src/compiler/test2.ts",
        "decl": "yarn js src/sdk/sdkGenerator.ts"
    },
    "dependencies": {
        "@rsuite/icons": "^1.0.2",
        "@types/vscode-webview": "^1.57.1",
        "@types/ws": "^8.5.4",
        "@vscode/webview-ui-toolkit": "^1.2.2",
        "cytoscape": "^3.23.0",
        "cytoscape-klay": "^3.1.4",
        "esbuild": "^0.17.12",
        "mobx": "^6.8.0",
        "mobx-react-lite": "^3.4.3",
        "nanoid": "^4.0.1",
        "node-fetch": "^3.3.1",
        "painterro": "^1.2.78",
        "pathe": "^1.1.0",
        "prettier": "^2.8.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rsuite": "^5.30.0",
        "scheduler": "^0.20.0",
        "ts-node": "^10.9.1",
        "ws": "^8.13.0"
    },
    "devDependencies": {
        "@types/cytoscape": "^3.19.9",
        "@types/cytoscape-klay": "^3.1.0",
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@types/prettier": "^2.7.2",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@types/vscode": "^1.77.0",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "@vitejs/plugin-react": "^3.1.0",
        "@vscode/test-electron": "^2.3.0",
        "eslint": "^8.36.0",
        "glob": "^8.1.0",
        "less": "^4.1.3",
        "mocha": "^10.2.0",
        "typescript": "^5.0.2",
        "vite": "^4.2.1"
    }
}
