{
    "name": "cushystudio-shell",
    "displayName": "[ALPHA] Cushy Studio",
    "author": {
        "name": "rvion",
        "url": "https://github.com/rvion/CushyStudio",
        "email": "vion.remi@gmail.com"
    },
    "icon": "public/CushyLogo.png",
    "publisher": "rvion",
    "repository": {
        "type": "git",
        "url": "https://github.com/rvion/CushyStudio"
    },
    "description": "üé® Generative Art Web-IDE and embeddable typescript SDK - based on ComfyUI",
    "license": "AGPL-3.0",
    "version": "0.0.99",
    "engines": {
        "vscode": "^1.77.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:**/*.cushy.ts"
    ],
    "main": "./main.js",
    "contributes": {
        "configuration": {
            "title": "CushyStudio",
            "properties": {
                "cushystudio.serverHostHTTP": {
                    "type": [
                        "string"
                    ],
                    "default": "http://localhost:8188",
                    "description": "your ComfyUI http (or https) server host"
                },
                "cushystudio.serverWSEndoint": {
                    "type": [
                        "string"
                    ],
                    "default": "ws://localhost:8188/ws",
                    "description": "your ComfyUI ws (or wss) endpoint"
                }
            }
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "cushy-studio",
                    "title": "Cushy Studio",
                    "icon": "public/CushyLogo.png"
                }
            ]
        },
        "views": {
            "cushy-studio": [
                {
                    "id": "cushystudio-servers",
                    "name": "Servers"
                },
                {
                    "id": "cushyrun",
                    "name": "Cushy Runs"
                },
                {
                    "id": "cushygraphs",
                    "name": "CushyGraph"
                }
            ]
        },
        "commands": [
            {
                "command": "cushystudio.helloWorld",
                "title": "cushystudio.helloWorld | Hello world"
            },
            {
                "command": "cushystudio.updateschema",
                "title": "cushystudio.updateschema | Connect"
            },
            {
                "command": "cushystudio.openwebview",
                "title": "cushystudio.openwebview | Open webview"
            },
            {
                "command": "cushystudio.import",
                "title": "cushystudio.import | import flow"
            },
            {
                "command": "cushystudio.importlegacy",
                "title": "cushystudio.importlegacy | import flow (preserve legacy IDs)"
            }
        ]
    },
    "scripts": {
        "üìå CONTRIBUTORS": "",
        "git:update-master": "git merge origin/master",
        "git:add-all": "git add . && git commit -m 'update' && git push",
        "git:push": "git push",
        "üìå SYNC": "",
        "syncServer": "HOST=localhost PORT=1234 YPERSISTENCE=./dbDir2 node ./node_modules/.bin/y-websocket",
        "üìå SHELL": "",
        "vscode:dev": "cross-env-shell code --extensionDevelopmentPath=${PWD}",
        "üìå BACK": "",
        "back:dev": "npm run jswatch src/back/main.ts",
        "back:build": "esbuild src/back/main.ts --bundle --outfile=dist/main2.js --format=esm --platform=node --packages=external",
        "back:build-watch": "npm run back:build -- --sourcemap --metafile=dist/meta.json --watch ",
        "back:sourcemap": "npm run back:build -- --sourcemap",
        "back:metafile": "npm run back:build -- --minify --metafile=dist/meta.json",
        "üìå DEBUG": "",
        "yolo:build": "esbuild src/core-back/xxxxx.ts --bundle --outfile=yolo/extension.js --format=cjs --platform=node",
        "üìå FRONT": "",
        "front:watch": "vite build --watch",
        "front:dev": "vite dev",
        "front:build": "vite build",
        "front:preview": "vite preview",
        "front:inspect": "npx vite-bundle-visualizer",
        "üìå CHECKS": "",
        "tsc:compile": "tsc -p ./",
        "tsc:watch": "tsc -watch -p ./",
        "tsc:check": "tsc",
        "üìå TESTS": "",
        "#pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js",
        "#lint": "eslint src --ext ts",
        "üìå RELEASE": "",
        "‚è∏Ô∏è vscode:prepublish": "npm run back:build -- --minify",
        "‚è∏Ô∏è bump": "bump patch --commit=v --tag=v --push",
        "publish": "vsce publish",
        "package": "vsce package",
        "üìå DEV": "",
        "js:old": "--experimental-specifier-resolution=node --experimental-modules",
        "js": "node --loader @swc-node/register/esm",
        "jswatch": "node --loader @swc-node/register/esm --watch",
        "üìå CORE": "",
        "sync": "yarn js src/core/ComfySpec.fetch.ts",
        "lib": "yarn js src/core/Comfy.gen.ts",
        "üìå COMPILER": "",
        "old:compile": "yarn js src/compiler/entry.gen.ts",
        "old:sample": "yarn js src/compiler/entry.fetch.ts",
        "old:test": "yarn js src/compiler/test.ts",
        "old:test2": "yarn js src/compiler/test2.ts",
        "decl": "yarn js src/typings/sdkGenerator.ts",
        "decl:local": "yarn js src/sdk/sdkGenerator.ts",
        "decl:list": "tsc -p tsconfig.decl.json --listFiles --noEmit | grep /Users/loco/dev/CushyStudio/src"
    },
    "dependencies": {
        "@rsuite/icons": "1.0.2",
        "@syncedstore/core": "^0.5.1",
        "@types/express": "^4.17.17",
        "@types/marked": "4.3.0",
        "@types/vscode-webview": "1.57.1",
        "@types/ws": "8.5.4",
        "@vscode/webview-ui-toolkit": "1.2.2",
        "chokidar": "^3.5.3",
        "cors": "^2.8.5",
        "cytoscape": "3.24.0",
        "cytoscape-klay": "3.1.4",
        "electron": "^24.4.0",
        "esbuild": "0.17.19",
        "express": "^4.18.2",
        "form-data": "4.0.0",
        "highlight.js": "^11.8.0",
        "marked": "5.0.2",
        "mobx": "6.9.0",
        "mobx-react-lite": "3.4.3",
        "nanoid": "4.0.2",
        "node-fetch": "3.3.1",
        "open": "^9.1.0",
        "pathe": "1.1.0",
        "PostCSS": "npm:postcss@^8.4.23",
        "prettier": "^2.8.8",
        "react": "18.2.0",
        "react-dnd": "^16.0.1",
        "react-dnd-html5-backend": "^16.0.1",
        "react-dom": "18.2.0",
        "react-zoom-pan-pinch": "^3.0.7",
        "rsuite": "5.33.1",
        "ws": "8.13.0",
        "y-leveldb": "^0.1.2",
        "y-websocket": "^1.5.0",
        "yet-another-react-lightbox": "3.8.3",
        "yjs": "^13.6.1"
    },
    "devDependencies": {
        "@swc-node/register": "^1.6.5",
        "@types/cors": "^2.8.13",
        "@types/cytoscape": "3.19.9",
        "@types/cytoscape-klay": "3.1.0",
        "@types/glob": "8.1.0",
        "@types/mocha": "10.0.1",
        "@types/node": "20.1.7",
        "@types/prettier": "2.7.2",
        "@types/react": "18.2.6",
        "@types/react-dom": "18.2.4",
        "@types/vscode": "1.78.0",
        "@typescript-eslint/eslint-plugin": "5.59.6",
        "@typescript-eslint/parser": "5.59.6",
        "@vitejs/plugin-react": "4.0.0",
        "@vscode/test-electron": "2.3.2",
        "autoprefixer": "^10.4.14",
        "cross-env": "^7.0.3",
        "eslint": "8.40.0",
        "glob": "10.2.4",
        "less": "4.1.3",
        "mocha": "10.2.0",
        "postcss": "^8.4.23",
        "tailwindcss": "^3.3.2",
        "typescript": "5.0.4",
        "vite": "4.3.7",
        "vsce": "2.15.0"
    }
}
